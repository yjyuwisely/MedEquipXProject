<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="pink.supply.mapper.SignUpMapper">

<insert id="logisticsJoin" parameterType="pink.supply.model.MemberVO">
insert into member
	(
	email
	, password
	, name
	)
values
	(
	#{email}
	, #{password}
	, #{name}
	)
</insert>

</mapper>
