<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="pink.supply.mapper.OrderHistoryMapper">

<select id="callOrderHistory" parameterType="pink.supply.model.OrderHistoryVO" resultType="pink.supply.model.OrderHistoryVO">
SELECT
	name
	, specifications
	, department 
	, quantity 
	, change_date 
	, in_charge
	, memo
FROM 
	released

</select>
</mapper>